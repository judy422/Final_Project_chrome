{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "permissions": ["storage", "activeTab", "nativeMessaging","scripting"],
  "background": {
    "service_worker": "background/background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "128": "images/open-book.png"
    }
  },
  "icons": {
    "128": "images/open-book.png"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
