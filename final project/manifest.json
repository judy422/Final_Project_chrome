
{
  "manifest_version": 2,
  "name": "實習小幫手",
  "version": "1.0",
  "description": "這是一個記錄實習資訊的書籤套件",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": ["storage","activeTab"]
  "web_accessible_resources": [
    "final project/**"  
  ]
}


